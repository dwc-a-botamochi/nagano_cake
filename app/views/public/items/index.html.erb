<h2>商品一覧</h2>
<div class="container">
  <div class="row">
    <% @items.each do |item| %>
        <div class = "col-3 mb-4">
        <%= link_to item_path(item) do %>
            <%= image_tag item.get_image(200,200) %>
            <p><%= item.name %></p>
            <p>￥<%= number_with_delimiter((item.price * 1.1).floor) %></p>
        <% end %>
        </div>
    <% end %>
  </div>
</div>